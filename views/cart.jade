						.modal-body
							form(action='customer-orders.html', method='post')
								.form-group
									input#email-modal.form-control(type='text', placeholder='email')
								.form-group
									input#password-modal.form-control(type='password', placeholder='password')
								p.text-center
									button.btn.btn-primary
										i.fa.fa-sign-in
										|  Log in
